#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";
        
        default_layer {
            // Minimal keymap for standalone trackpad
            // This single "none" binding matches the 1x1 matrix transform
            bindings = 
                &none
            >;
        };
    };
};